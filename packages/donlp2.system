;;; -*- Mode: Lisp -*-

(defun donlp2-compiler (filename &rest args)
  (apply #'f2cl:f2cl-compile filename args))

(mk:define-language :f2cl
    :compiler #'donlp2-compiler
    :source-extension "f")

(mk:defsystem ex-antenna1
  :source-pathname (make-pathname :directory (pathname-directory *load-pathname*))
  :depends-on ("donlp2")
  :components
  ((:module donlp2
	    :source-extension "f"
	    :language :f2cl
	    :components
	    (
	     (:file "antenna1"
		    :compiler-options (:declare-common t))))))

(mk:defsystem ex-hs101
  :source-pathname (make-pathname :directory (pathname-directory *load-pathname*))
  :depends-on ("donlp2")
  :components
  ((:module donlp2
	    :source-extension "f"
	    :language :f2cl
	    :components
	    (
	     (:file "hs101"
		    :compiler-options (:declare-common t))))))

(mk:defsystem donlp2
  :source-pathname (make-pathname :directory (pathname-directory *load-pathname*))
  :components
  ((:module donlp2
	    :source-extension "f"
	    :language :f2cl
	    :components
	    (
	     (:file "support.lisp"
